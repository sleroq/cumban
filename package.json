{
  "name": "bases-kanban",
  "version": "1.0.0",
  "description": "Kanban view for Obsidian Bases.",
  "main": "main.js",
  "scripts": {
    "dev": "bun esbuild.config.mjs",
    "typecheck": "tsc -noEmit -skipLibCheck",
    "build": "bun run typecheck && bun esbuild.config.mjs production",
    "lint": "eslint ./src",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "kanban",
    "bases"
  ],
  "author": "sleroq",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.3",
    "esbuild-svelte": "^0.9.2",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "obsidian": "1.12.0",
    "svelte": "^5.28.2",
    "svelte-check": "^4.2.1",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
